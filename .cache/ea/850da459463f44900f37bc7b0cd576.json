nents/Carousel.tsx","name":null,"original":{"line":13,"column":54},"generated":{"line":75,"column":0}},{"source":"components/Carousel.tsx","name":null,"original":{"line":64,"column":0},"generated":{"line":77,"column":0}},{"source":"components/Carousel.tsx","name":null,"original":{"line":64,"column":0},"generated":{"line":77,"column":7}},{"source":"components/Carousel.tsx","name":null,"original":{"line":64,"column":0},"generated":{"line":77,"column":8}},{"source":"components/Carousel.tsx","name":null,"original":{"line":64,"column":0},"generated":{"line":77,"column":15}},{"source":"components/Carousel.tsx","name":null,"original":{"line":64,"column":15},"generated":{"line":77,"column":18}},{"source":"components/Carousel.tsx","name":null,"original":{"line":64,"column":0},"generated":{"line":77,"column":26}}],"sources":{"components/Carousel.tsx":"import React from \"react\";\nimport { Photo } from \"@frontendmasters/pet\";\n\ninterface IProps {\n  media: Photo[];\n}\n\ninterface IState {\n  active: number;\n  photos: string[];\n}\n\nclass Carousel extends React.Component<IProps, IState> {\n  public state = {\n    photos: [],\n    active: 0,\n  };\n  public static getDerivedStateFromProps({ media }: IProps) {\n    let photos = [\"http://placecorgi.com/600/600\"];\n\n    if (media.length) {\n      photos = media.map(({ large }) => large);\n    }\n\n    return { photos };\n  }\n\n  public handleIndexClick = (e: React.MouseEvent<HTMLElement>) => {\n    if (!(e.target instanceof HTMLElement)) {\n      return;\n    }\n\n    if (e.target.dataset.index) {\n      this.setState({\n        active: +e.target.dataset.index,\n      });\n    }\n  };\n\n  public render() {\n    const { photos, active } = this.state;\n\n    return (\n      <div className=\"carousel\">\n        <img src={photos[active]} alt=\"animal\" />\n        <div className=\"carousel-smaller\">\n          {photos.map((photo, index) => (\n            // eslint-disable-next-line\n            <img\n              key={photo}\n              onClick={this.handleIndexClick}\n              data-index={index}\n              src={photo}\n              className={index === active ? \"active\" : \"\"}\n              alt=\"animal thumbnail\"\n            />\n          ))}\n        </div>\n      </div>\n    );\n  }\n}\n\nexport default Carousel;\n"},"lineCount":null}},"error":null,"hash":"9d4f44811bf6d464139b93a2e7d413c5","cacheData":{"env":{}}}