,"source":"components/SearchParams.js","original":{"line":68,"column":6}},{"generated":{"line":90,"column":4},"source":"components/SearchParams.js","original":{"line":36,"column":4}},{"generated":{"line":90,"column":5},"source":"components/SearchParams.js","original":{"line":35,"column":2}},{"generated":{"line":91,"column":0},"source":"components/SearchParams.js","original":{"line":71,"column":1}},{"generated":{"line":91,"column":1},"source":"components/SearchParams.js","original":{"line":7,"column":0}},{"name":"SearchParams","generated":{"line":93,"column":15},"source":"components/SearchParams.js","original":{"line":73,"column":15}}],"sources":{"components/SearchParams.js":"import React, { useState, useEffect, useContext } from \"react\";\nimport pet, { ANIMALS } from \"@frontendmasters/pet\";\nimport useDropdown from \"./hooks/useDropdown\";\nimport Results from \"./Results\";\nimport ThemeContext from \"./hooks/ThemeContext\";\n\nconst SearchParams = () => {\n  const [location, setLocation] = useState(\"Seattle, WA\");\n  const [breeds, setBreeds] = useState([]);\n  const [animal, AnimalDropdown] = useDropdown(\"Animal\", \"dog\", ANIMALS);\n  const [breed, BreedDropdown, setBreed] = useDropdown(\"Breed\", \"\", breeds);\n  const [pets, setPets] = useState([]);\n  const [theme, setTheme] = useContext(ThemeContext);\n\n  async function requestPets() {\n    const { animals } = await pet.animals({\n      location,\n      breed,\n      type: animal,\n    });\n\n    setPets(animals || []);\n  }\n\n  useEffect(() => {\n    setBreeds([]);\n    setBreed(\"\");\n\n    pet.breeds(animal).then(({ breeds: apiBreeds }) => {\n      const breedStrings = apiBreeds.map(({ name }) => name);\n      setBreeds(breedStrings);\n    }, console.error);\n  }, [animal, setBreed, setBreeds]);\n\n  return (\n    <div className=\"search-params\">\n      <form\n        onSubmit={(e) => {\n          e.preventDefault();\n          requestPets();\n        }}\n      >\n        <label htmlFor=\"location\">\n          Location\n          <input\n            id=\"location\"\n            value={location}\n            placeholder=\"Location\"\n            onChange={(e) => setLocation(e.target.value)}\n          />\n        </label>\n        <AnimalDropdown />\n        <BreedDropdown />\n        <label htmlFor=\"theme\">\n          Theme\n          <select\n            value={theme}\n            onChange={(e) => setTheme(e.target.value)}\n            onBlur={(e) => setTheme(e.target.value)}\n          >\n            <option value=\"peru\">Peru</option>\n            <option value=\"darkblue\">Dark Blue</option>\n            <option value=\"purple\">Purple</option>\n          </select>\n        </label>\n        <button style={{ backgroundColor: theme }}>Submit</button>\n      </form>\n      <Results pets={pets} />\n    </div>\n  );\n};\n\nexport default SearchParams;\n"},"lineCount":null}},"error":null,"hash":"9fcd0cdf21285be305af0fa4a1ac8f2b","cacheData":{"env":{}}}